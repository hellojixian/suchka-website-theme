  button, .register-button{
    border-radius: 0px;
    padding: 12px 40px;
    background-color: #cc0000;
    text-transform: uppercase;
    color:#fff;
    position: relative;
    overflow: hidden;
    border:none;
    letter-spacing: 2px;
    font-size: 14px;
    &:before{
      content: '';
      position: absolute;
      opacity: 0;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      border-top: 1px solid $color-main2;
      border-bottom: 1px solid $color-main2;
      transform: translateX(-100%);
      transition: all 0.3s ease-out;
    }
    &:after{
      content: '';
      position: absolute;
      opacity: 0;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      border-right: 1px solid $color-main2;
      border-left: 1px solid $color-main2;
      transform: translateY(-100%);
      transition: all 0.3s ease-out;

    }

    &:hover{
      background-color: #5b0000;
      color: #fff;
      box-shadow: 0 5px 15px rgba(0, 0, 0, .08);
      &:before{
          transform: translateX(0);
          opacity: 1;

        }
        &:after{
          transform: translateY(0);
          opacity: 1;

        }
    }
  }
   .forgot_pass{
