<footer class="footer_v1">
  <div class="container container-casa">    
    <div class="row align-items-center">
      <div class="col-lg-3 col-md-3 text-lg-left">
        <div class="logo-left">
          {% if settings.logo_svg1 !=blank %}
          <div class="logosvg" style="max-width:{{settings.fv1_main_logo_maxwidth}}px;">
            <a class="d-block" href="{{shop.url}}">{{settings.logo_svg1}}</a>
          </div>          
          {% else %}          	
          <a href="{{shop.url}}" class="logo">
            {% if settings.fv1_main_logo != blank %}
            <img src="{{settings.fv1_main_logo | img_url:'master'}}"  width="{{settings.fv1_main_logo_maxwidth}}" alt="logo">
            {% else %}
            <img src="//placehold.it/112x16" width="{{settings.fv1_main_logo_maxwidth}}" alt="logo">
            {% endif %}
          </a>
          {% endif %}
        </div>
      </div>

      <div class="col-lg-6 col-md-6 text-center">
        <div class="menu">
          <ul class="list-inline list-unstyled mb-0">
            {% for link in linklists[settings.menu_fv1].links %}
            <li class="list-inline-item mr-0"><a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a></li>
            {% endfor %}
          </ul>
        </div>
      </div>

      <div class="col-lg-3 col-md-3 text-lg-right">
        <div class="casano-socials">
          <ul class="list-inline list-unstyled mb-0">
            {% if settings.icon_sc_fv1_1 !=blank %}
            <li class="list-inline-item mr-0 mb-0">
              <a href="{{settings.url_sc_fv1_1}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv1_1}}"></i></a>
            </li>
            {% endif %}
            {% if settings.icon_sc_fv1_2 !=blank %}
            <li class="list-inline-item mr-0 mb-0">
              <a href="{{settings.url_sc_fv1_2}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv1_2}}"></i></a>
            </li>
            {% endif %}
            {% if settings.icon_sc_fv1_3 !=blank %}
            <li class="list-inline-item mr-0 mb-0">
              <a href="{{settings.url_sc_fv1_3}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv1_3}}"></i></a>
            </li>
            {% endif %}
            {% if settings.icon_sc_fv1_4 !=blank %}
            <li class="list-inline-item mr-0 mb-0">
              <a href="{{settings.url_sc_fv1_4}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv1_4}}"></i></a>
            </li>
            {% endif %}        	

          </ul>
        </div>
      </div> 

    </div>
  </div> 
</footer>